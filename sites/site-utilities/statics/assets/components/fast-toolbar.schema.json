{
  "$schema": "http://json-schema.org/schema#",
  "$id": "fast-toolbar",
  "id": "fast-toolbar",
  "title": "Toolbar",
  "description": "The FAST Toolbar element",
  "type": "object",
  "version": 1,
  "mapsToTagName": "fast-toolbar",
  "properties": {
    "orientation": {
      "enum": [
        "horizontal",
        "vertical"
      ],
      "default": "horizontal",
      "title": "Orientation",
      "description": "The visual orientation of the toolbar",
      "mapsToAttribute": "orientation",
      "type": "string"
    },
    "Slot": {
      "title": "Default slot",
      "description": "The toolbar content",
      "mapsToSlot": "",
      "@microsoft/fast-tooling/dictionary-link": true,
      "pluginId": "@microsoft/fast-tooling/linked-data",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "title": "The ID of the data corresponding to a dictionary key",
            "type": "string"
          },
          "dataLocation": {
            "title": "The location of the data using lodash path syntax",
            "type": "string"
          }
        }
      }
    },
    "SlotLabel": {
      "title": "Default slot",
      "description": "The toolbar content",
      "mapsToSlot": "label",
      "@microsoft/fast-tooling/dictionary-link": true,
      "pluginId": "@microsoft/fast-tooling/linked-data",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "title": "The ID of the data corresponding to a dictionary key",
            "type": "string"
          },
          "dataLocation": {
            "title": "The location of the data using lodash path syntax",
            "type": "string"
          }
        }
      }
    }
  }
}